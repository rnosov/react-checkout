exports[`StripeForm renders a initial view 1`] = `"<div class=\"simpleform-fadeIn\"><p>Please pay for your order</p><form novalidate=\"\" class=\"m-x-1 text-xs-left\" style=\"overflow: hidden;\"><noscript>Please enable javascript in order to use this form.</noscript><div class=\"form-group row\"><label class=\"col-xs-12 text-sm-right col-form-label col-sm-2\">*Name</label><div class=\"col-xs-12 col-sm-5\"><input type=\"text\" class=\"form-control\" value=\"\" data-id=\"0\" placeholder=\"John Doe\"></div><!-- react-empty: 9 --></div><div class=\"form-group row\"><label class=\"col-xs-12 text-sm-right col-form-label col-sm-2\">*Email</label><div class=\"col-xs-12 col-sm-5\"><input type=\"email\" class=\"form-control\" value=\"\" data-id=\"1\" placeholder=\"john@example.com\"></div><!-- react-empty: 14 --></div><div class=\"form-group row\"><label class=\"col-xs-12 text-sm-right col-form-label col-sm-2\">Phone</label><div class=\"col-xs-12 col-sm-5\"><input type=\"tel\" class=\"form-control\" value=\"\" data-id=\"2\" placeholder=\"+44 207 123 4567\"></div><!-- react-empty: 19 --></div><div class=\"form-group row\"><label class=\"col-xs-12 text-sm-right col-form-label col-sm-2\">*Card Number</label><div class=\"col-xs-12 col-sm-5\"><input type=\"tel\" class=\"form-control\" value=\"\" data-id=\"3\" autocomplete=\"cc-number\" placeholder=\"4242 4242 4242 4242\"><small class=\"form-text text-muted\">Long number on the front of your card</small></div><!-- react-empty: 25 --></div><div class=\"form-group row\"><label class=\"col-xs-12 text-sm-right col-form-label col-sm-2\">*CVC</label><div class=\"col-xs-12 col-sm-5\"><input type=\"number\" class=\"form-control\" value=\"\" data-id=\"4\" autocomplete=\"cc-csc\" placeholder=\"123\"><small class=\"form-text text-muted\">The 3 digits to the right of the signature strip located on the back of your card</small></div><!-- react-empty: 31 --></div><div class=\"form-group row\"><label class=\"col-xs-12 text-sm-right col-form-label col-sm-2\">*Expiry Date</label><div class=\"col-xs-12 col-sm-5\"><input type=\"tel\" class=\"form-control\" value=\"\" data-id=\"5\" autocomplete=\"cc-exp\" placeholder=\"10/17\"></div><!-- react-empty: 36 --></div><div class=\"form-group row\"><label class=\"col-xs-12 text-sm-right col-form-label col-sm-2\">*Address</label><div class=\"col-xs-12 col-sm-5\"><textarea class=\"form-control\" data-id=\"6\" rows=\"6\" placeholder=\"1 Chapel Hill, Heswall, BOURNEMOUTH, UK, BH1 1AA\"></textarea><small class=\"form-text text-muted\">The address where your order will be shipped</small></div><!-- react-empty: 42 --></div><div class=\"form-group row\"><div class=\"col-xs-12 col-sm-2\"><small class=\"form-text text-muted text-sm-right form-control-static\"><!-- react-text: 46 -->*<!-- /react-text --><!-- react-text: 47 -->Mandatory fields<!-- /react-text --></small></div><div class=\"col-xs-12 form-inline col-sm-8\"><div class=\"form-group has-danger\"><button type=\"submit\" class=\"btn btn-outline-primary\">Submit</button><div class=\"text-muted form-control-static\">&nbsp;&nbsp; </div><!-- react-empty: 52 --></div></div></div></form></div>"`;
